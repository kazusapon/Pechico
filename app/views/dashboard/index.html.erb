<div class="content-wrapper">
  <section class="content-header">
    <div class="container-fluid">
      <h4 class="m-0 text-dark">ダッシュボード</h4>
    </div>
  </section>

  <section class="content">
    <div class="container-fluid">
      <div class="card card-primary card-outline card-outline-tabs">
        <div class="card-header border-bottom-0 p-0 pt-2">
          <ul class="nav nav-tabs" role="tablist">
            <li class="nav-item">
              <a class="nav-link active" id="whole-tab" data-bs-toggle="tab" href="#whole" role="tab" 
                 aria-controls="whole-tab" aria-selected="true">全体</a>
            </li>
            <li>
              <a class="nav-link" id="parsonal-tab" data-bs-toggle="tab" href="#parsonal" role="tab"
                 aria-controls="parsonal-tab" aria-selected="false">ユーザ毎</a>
            </li>
          </ul>
        </div>

        <div class="card-body">
          <div class="tab-content">
            <%= render partial: 'whole_tab_item' %>
            <%= render partial: 'parsonal_tab_item' %>
          </div>
        </div>
      </div>
    </div>
  </section>
</div>

<div id="modal_area">
</div>